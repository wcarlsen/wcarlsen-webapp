{
  "name": "wcarlsen-webapp",
  "profile": "default",
  "regions": [
    "eu-west-1"
  ],
  "type": "static",
  "static": {
    "dir": "build"
  },
  "lambda": {
    "memory": 1024
  },
  "stages": {
    "staging": {
      "hooks": {
        "build": "PUBLIC_URL=/staging npm run build"
      }
    },
    "production": {
      "hooks": {
        "build": "PUBLIC_URL=/production npm run build"
      }
    }
  },
  "hooks": {
    "build": "npm run build"
  },
  "error_pages": {
    "variables": {
      "support_email": "carlsenwilli@gmail.com",
      "color": "#228ae6"
    }
  },
  "clean": "rm -fr build"
}