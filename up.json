{
  "name": "wcarlsen-webapp",
  "profile": "default",
  "regions": [
    "eu-west-1"
  ],
  "static": {
    "dir": "build"
  },
  "stages": {
    "staging": {
      "hooks": {
        "build": "PUBLIC_URL=/staging npm build"
      }
    },
    "production": {
      "hooks": {
        "build": "PUBLIC_URL=/production npm build"
      }
    }
  },
  "hooks": {
    "build": "npm build"
  },
  "clean": "rm -fr build"
}